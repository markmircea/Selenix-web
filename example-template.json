{
  "metadata": {
    "id": "example-product-scraper",
    "title": "Product Data Scraper",
    "description": "Extract product information from e-commerce websites including prices, descriptions, ratings, and more.",
    "category": "data-scraping",
    "tags": ["E-commerce", "Products", "Pricing"],
    "version": "1.0",
    "created_at": "2024-06-12",
    "created_by": "Selenix Team"
  },
  "workflow": {
    "steps": [
      {
        "id": 1,
        "type": "navigate",
        "action": "goto",
        "url": "{{target_url}}",
        "description": "Navigate to target website",
        "settings": {
          "timeout": 30000,
          "wait_for_load": true
        }
      },
      {
        "id": 2,
        "type": "wait",
        "action": "wait_for_element",
        "selector": "{{main_content_selector}}",
        "timeout": 10000,
        "description": "Wait for page to load"
      },
      {
        "id": 3,
        "type": "extract",
        "action": "scrape_data",
        "selectors": {
          "title": "{{title_selector}}",
          "description": "{{description_selector}}",
          "price": "{{price_selector}}",
          "rating": "{{rating_selector}}",
          "availability": "{{availability_selector}}"
        },
        "description": "Extract product data from page"
      },
      {
        "id": 4,
        "type": "output",
        "action": "save_data",
        "format": "csv",
        "filename": "scraped_products_{{timestamp}}.csv",
        "description": "Save extracted data to CSV file"
      }
    ],
    "variables": {
      "target_url": "https://example-store.com/product/example",
      "main_content_selector": "main",
      "title_selector": "h1.product-title",
      "description_selector": ".product-description",
      "price_selector": ".price-current",
      "rating_selector": ".rating-stars",
      "availability_selector": ".stock-status"
    },
    "conditions": [
      {
        "id": 1,
        "type": "element_exists",
        "selector": "{{price_selector}}",
        "description": "Check if price element exists"
      }
    ]
  },
  "settings": {
    "execution": {
      "delay_between_steps": 1000,
      "timeout_per_step": 30000,
      "retry_failed_steps": true,
      "max_retries": 3
    },
    "browser": {
      "headless": false,
      "window_size": {
        "width": 1280,
        "height": 720
      },
      "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
    },
    "output": {
      "save_screenshots": true,
      "log_level": "info",
      "export_format": "json"
    },
    "notifications": {
      "on_completion": true,
      "on_error": true,
      "email_notifications": false
    }
  }
}
